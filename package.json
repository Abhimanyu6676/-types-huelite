{
  "name": "@keystonejs/example-projects-todo",
  "description": "An example KeystoneJS project showcasing a simple Todo List with a Keystone StaticApp front-end.",
  "private": true,
  "version": "5.0.7",
  "author": "The KeystoneJS Development Team",
  "repository": "https://github.com/keystonejs/keystone/tree/master/packages/create-keystone-app/example-projects/todo",
  "homepage": "https://github.com/keystonejs/keystone",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "_build": "tsc",
    "startTS": "ts-node src/Pindex.ts",
    "dev": "cross-env NODE_ENV=development DISABLE_LOGGING=true keystone dev",
    "build": "cross-env NODE_ENV=production keystone build /build/src/index.js",
    "start": "cross-env NODE_ENV=production keystone start",
    "nodemon": "cross-env NODE_ENV=development DISABLE_LOGGING=true npx nodemon src/Pindex.ts",
    "create-tables": "keystone create-tables"
  },
  "dependencies": {
    "@keystonejs/adapter-mongoose": "^8.1.0",
    "@keystonejs/app-admin-ui": "^6.0.0",
    "@keystonejs/app-graphql": "^5.1.7",
    "@keystonejs/app-static": "^5.1.2",
    "@keystonejs/fields": "^10.0.0",
    "@keystonejs/keystone": "^9.0.0",
    "aedes": "^0.42.6",
    "aedes-logging": "^2.0.1",
    "aedes-persistence-mongodb": "^8.1.2",
    "connect-mongo": "^3.2.0",
    "cross-env": "^7.0.0",
    "express-session": "^1.17.1",
    "mqtt": "^4.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "typescript": "^4.0.3",
    "ts-node": "^9.0.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.8"
  }
}
